    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "chai": "^6.0.1",
    "eslint": "^9.13.0",
    "markdownlint": "^0.38.0",
    "markdownlint-cli": "^0.45.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7",
    "proxyquire": "^2.1.3",
    "sinon": "^21.0.0",
