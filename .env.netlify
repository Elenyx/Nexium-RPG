# Environment variables for Netlify
# IMPORTANT: Replace these with your actual values and add in Netlify settings

# Discord OAuth settings
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_CLIENT_SECRET=your_client_secret_here
# IMPORTANT: Must match exactly what you register in Discord Developer Portal
DISCORD_REDIRECT_URI=https://nexium-rpg.win/.netlify/functions/api/auth/discord/callback

# Set to 'production' in Netlify deployment
NODE_ENV=production

# Port for Netlify Dev (local development)
PORT=8888
